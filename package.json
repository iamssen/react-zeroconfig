    "watch": "yarn run clean && sh foreach.sh packages 'yarn run watch'",
    "start:my-app-test": "cd samples/my-app-test && yarn run app:start --port 3110",
    "e2e": "bash test/e2e.sh",
    "test": "jest --colors",
    "coverage": "jest --coverage",
    "lint": "sh foreach.sh packages 'yarn run lint'",
    "postinstall": "yarn build",
    "download": "node scripts/get-node-api-list.js",
    "foreach": "sh foreach.sh packages 'yarn run lint'"
