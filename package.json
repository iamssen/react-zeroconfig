{
  "private": true,
  "scripts": {
    "build": "rocket-punch build",
    "publish": "npm test && rocket-punch build && rocket-punch publish",
    "publish:local": "rocket-punch build && rocket-punch publish --tag canary --registry $LOCAL_NPM_REGISTRY",
    "view": "rocket-punch view",
    "doctor": "rocket-punch doctor",
    "test": "jest --runInBand --colors --forceExit --detectOpenHandles",
    "coverage": "jest --runInBand --forceExit --detectOpenHandles --coverage",
    "e2e": "npm run build && node test/updateVerdaccioYaml.js && bash test/e2e.sh",
    "format": "prettier --write \"src/{**/*,*}/*.{ts,tsx}\"",
    "lint": "eslint \"src/{**/*,*}/*.{ts,tsx}\"",
    "test-run:start": "ts-node -r tsconfig-paths/register src/@rocket-scripts/web/__run__/start.run.ts",
    "test-run:proxy:start": "ts-node -r tsconfig-paths/register src/@rocket-scripts/web/__run__/start.proxy.run.ts",
    "test-run:build": "ts-node -r tsconfig-paths/register src/@rocket-scripts/web/__run__/build.run.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "markdown-source-import \"{,!(node_modules)/**/}*.md\" --git-add && lint-staged"
    }
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "prettier --write",
      "eslint",
      "jest --runInBand --colors --findRelatedTests"
    ]
