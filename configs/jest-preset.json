{
  "transform": {
    ".(ts|tsx)": "ts-jest"
  },
  "testMatch": [
    "**/__tests__/**/*.ts?(x)",
    "**/?(*.)(spec|test).ts?(x)"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "moduleDirectories": [
    "<rootDir>/node_modules",
    "<rootDir>/src/_modules",
    "<rootDir>/src"
  ]
}